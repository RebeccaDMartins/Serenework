{
  "expo": {
    "name": "Serenework",
    "slug": "serenework",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/Icon-Serene-Work.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/Icon-Serene-Work.png",
        "backgroundColor": "#6A4BBC"
      }
    },
    "web": {
      "favicon": "./assets/img/Icon-Serene-Work.png"
    },
    "plugins": [
      "expo-asset"
    ]
  }
}
